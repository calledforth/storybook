# Replicate API Token
# Get your token from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_api_token_here

# Replicate username or org for model ownership
REPLICATE_OWNER=your_username

# Google Gemini API key
GEMINI_API_KEY=your_gemini_key

# Inpainting feature flag (default true). Set to "false" to use legacy compositing.
USE_INPAINTING=true

